[mysqld]
# Keep log files smaller to reduce initial disk usage
aria_log_file_size = 134217728   # 128M
innodb_log_file_size = 134217728 # 128M

# Conservative memory settings for dev containers
innodb_buffer_pool_size = 64M
max_heap_table_size = 16M

# Use tmp inside container
tmpdir = /tmp

# Reduce binary logging (if not needed)
skip-log-bin

